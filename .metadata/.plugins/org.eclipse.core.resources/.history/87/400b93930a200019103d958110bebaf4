package java_basics;

public class LearningArrays {

	public static void main(String[] args) {
	//	int values;
	//	values = 100;
		
		int [] values = new int[100];
		values[0] = 1000;
		values[99]=93432;
		
		System.out.println(values[0]);
		System.out.println(values[99]);
		System.out.println(values[1000]);


		
	}

}
