package java_basics;

public class MyUtils {
	
	public static void printSomeJunk(String argument) {
		
		System.out.println("Blah blah "+ argument);
	}

	
	
	private static int printSomeJunk(int argument) {
	
		return argument;
	}
	
	
	public static void sum2Numbers(int firstArg, int secondArg) {
		
		System.out.println(firstArg+secondArg);

		
	}
	
	
	public static int add10(int someArg) {
		
		int result = someArg + 10;
		
		return result;
		//System.out.println(someArg+10);

		
	}
	

}
